{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "miauchat",
  "version": "0.1.0",
  "identifier": "com.miauchat.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "miauchat",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/256x256.png",
      "icons/icon.png",
      "icons/miauico_7fs_icon.ico"
    ]
  }
}
